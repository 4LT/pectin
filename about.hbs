{{#each licenses}}
{
    name "{{name}}"
    text "{{text}}"
    usedBy {
        {{#each used_by}}
        {
            href {{#if crate.repository}}
                    "{{crate.repository}}"
                {{else}}
                    "https://crates.io/crates/{{crate.name}}"
                {{/if}}
            crate "{{crate.name}}"
            version "{{crate.version}}"
        }
        {{/each}}
    }
}
{{/each}}
